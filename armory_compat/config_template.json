{
  "_description": "Config to run ${ATTACK} on ${DEFENSE} with ${METRIC} metric",
  "adhoc": null,
  "attack": {
      "knowledge": "white",
      "kwargs": {
        "defense": "${DEFENSE}",
        "attack": "${ATTACK}"
      },
      "module": "armory_compat.wrappers",
      "name": "AttackWrapper",
      "use_label": true
  },
  "dataset": {
      "batch_size": 32,
      "module": "armory_compat.wrappers",
      "name": "cifar10",
      "framework": "numpy"
  },
  "defense": null,
  "metric": {
      "means": true,
      "perturbation": "${METRIC}",
      "record_metric_per_sample": true,
      "task": [
          "categorical_accuracy"
      ]
  },
  "model": {
      "fit": false,
      "fit_kwargs": {
      },
      "model_kwargs": {
        "attack": "${ATTACK}",
        "defense": "${DEFENSE}"
      },
      "module": "armory_compat.wrappers",
      "name": "get_art_model",
      "weights_file": null,
      "wrapper_kwargs": {}
  },
  "scenario": {
      "kwargs": {},
      "module": "armory.scenarios.image_classification",
      "name": "ImageClassificationTask"
  },
  "sysconfig": {
      "docker_image": "twosixarmory/${FRAMEWORK}:${VERSION}",
      "external_github_repo": null,
      "gpus": "0",
      "use_gpu": true
  }
}
